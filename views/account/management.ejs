<div class="account-management">
  <h1><%= title %></h1>

 
  <% if (errors && errors.length > 0) { %>
    <div class="error-container">
      <ul>
        <% errors.forEach(error => { %>
          <li><%= error.msg %></li>
        <% }) %>
      </ul>
    </div>
  <% } %>

  <p>You're logged in.</p>
</div>
